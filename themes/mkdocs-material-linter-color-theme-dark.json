{
  "name": "MkDocs Material Linter (Dark)",
  "type": "dark",
  "tokenColors": [
    {
      "name": "MkDocs Admonitions",
      "scope": [
        "mkdocs.admonition.marker",
        "mkdocs.admonition.type",
        "mkdocs.admonition.modifier",
        "mkdocs.admonition.title"
      ],
      "settings": {
        "foreground": "#F59E0B"
      }
    },
    {
      "name": "MkDocs Front Matter",
      "scope": [
        "mkdocs.frontmatter.delimiter",
        "mkdocs.frontmatter.key",
        "mkdocs.frontmatter.value"
      ],
      "settings": {
        "foreground": "#2DD4BF"
      }
    },
    {
      "name": "MkDocs Headings",
      "scope": [
        "mkdocs.heading"
      ],
      "settings": {
        "foreground": "#60A5FA",
        "fontStyle": "bold"
      }
    },
    {
      "name": "MkDocs Blockquotes",
      "scope": [
        "mkdocs.blockquote",
        "markup.quote.markdown",
        "punctuation.definition.blockquote.markdown"
      ],
      "settings": {
        "foreground": "#9CA3AF",
        "background": "rgba(42, 47, 56, 0.6)"
      }
    },
    {
      "name": "MkDocs Annotations",
      "scope": [
        "mkdocs.annotation.block",
        "mkdocs.annotation.marker"
      ],
      "settings": {
        "foreground": "#38BDF8"
      }
    },
    {
      "name": "MkDocs Buttons",
      "scope": [
        "mkdocs.button.class"
      ],
      "settings": {
        "foreground": "#22C55E"
      }
    },
    {
      "name": "MkDocs Code Fences",
      "scope": [
        "mkdocs.code.fence"
      ],
      "settings": {
        "foreground": "#94A3B8"
      }
    },
    {
      "name": "MkDocs Code Blocks Background",
      "scope": [
        "markup.fenced_code.block.markdown",
        "markup.code.block.markdown"
      ],
      "settings": {
        "background": "#0b1220",
        "foreground": "#e2e8f0"
      }
    },
    {
      "name": "MkDocs Content Tabs",
      "scope": [
        "mkdocs.tab.marker",
        "mkdocs.tab.title"
      ],
      "settings": {
        "foreground": "#A78BFA"
      }
    },
    {
      "name": "MkDocs Data Tables",
      "scope": [
        "mkdocs.table.row",
        "mkdocs.table.separator"
      ],
      "settings": {
        "foreground": "#D1C4B2",
        "background": "rgba(58, 45, 36, 0.6)"
      }
    },
    {
      "name": "MkDocs Diagrams",
      "scope": [
        "mkdocs.diagram.fence"
      ],
      "settings": {
        "foreground": "#22D3EE"
      }
    },
    {
      "name": "MkDocs Footnotes",
      "scope": [
        "mkdocs.footnote.ref",
        "mkdocs.footnote.def"
      ],
      "settings": {
        "foreground": "#C084FC"
      }
    },
    {
      "name": "MkDocs Formatting",
      "scope": [
        "mkdocs.format.critic",
        "mkdocs.format.comment",
        "mkdocs.format.delete",
        "mkdocs.format.insert",
        "mkdocs.format.mark",
        "mkdocs.format.underline",
        "mkdocs.format.keys"
      ],
      "settings": {
        "foreground": "#F472B6"
      }
    },
    {
      "name": "MkDocs Underline",
      "scope": [
        "mkdocs.format.underline"
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "name": "MkDocs Inline Bold/Italic",
      "scope": [
        "mkdocs.inline.bolditalic"
      ],
      "settings": {
        "foreground": "#E5E7EB",
        "fontStyle": "bold italic"
      }
    },
    {
      "name": "MkDocs Inline Bold",
      "scope": [
        "mkdocs.inline.bold"
      ],
      "settings": {
        "foreground": "#E5E7EB",
        "fontStyle": "bold"
      }
    },
    {
      "name": "MkDocs Inline Italic",
      "scope": [
        "mkdocs.inline.italic"
      ],
      "settings": {
        "foreground": "#CBD5E1",
        "fontStyle": "italic"
      }
    },
    {
      "name": "MkDocs Grids",
      "scope": [
        "mkdocs.grid.container",
        "mkdocs.grid.card"
      ],
      "settings": {
        "foreground": "#A3E635"
      }
    },
    {
      "name": "MkDocs Icons and Emojis",
      "scope": [
        "mkdocs.icon"
      ],
      "settings": {
        "foreground": "#F59E0B"
      }
    },
    {
      "name": "MkDocs Images",
      "scope": [
        "mkdocs.image.inline",
        "mkdocs.image.caption"
      ],
      "settings": {
        "foreground": "#34D399"
      }
    },
    {
      "name": "MkDocs Lists",
      "scope": [
        "mkdocs.list.marker",
        "mkdocs.list.task",
        "mkdocs.list.definition"
      ],
      "settings": {
        "foreground": "#60A5FA"
      }
    },
    {
      "name": "MkDocs Math",
      "scope": [
        "mkdocs.math",
        "mkdocs.math.content"
      ],
      "settings": {
        "foreground": "#F87171",
        "fontStyle": "bold"
      }
    },
    {
      "name": "MkDocs Tooltips",
      "scope": [
        "mkdocs.tooltip",
        "mkdocs.tooltip.abbr"
      ],
      "settings": {
        "foreground": "#2DD4BF"
      }
    }
  ]
}
